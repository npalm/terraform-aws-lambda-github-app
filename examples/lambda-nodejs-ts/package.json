    "@typescript-eslint/parser": "^2.30.0",
    "@zeit/ncc": "^0.22.1",
    "aws-sdk": "^2.645.0",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "express": "^4.17.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "crypto": "^1.0.1"
  }
