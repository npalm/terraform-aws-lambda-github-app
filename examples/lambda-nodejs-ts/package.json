{
  "name": "github-runner-lambda-agent-webhook",
  "version": "1.0.0",
  "main": "lambda.ts",
  "license": "MIT",
  "scripts": {
    "start": "ts-node-dev src/local.ts",
    "test": "NODE_ENV=test jest",
    "lint": "yarn eslint --ext ts,tsx src",
